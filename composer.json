{
    "name": "viral-vector/minimvc-di",
    "description": "dependency injection & service locator",
    "keywords": ["framework", "php", "mvc", "quick", "slim", "di", "service locator"],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "viral-vector",
            "email": "vectorviral@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "/var/www/MiniMVCFrame"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "files": [
            "src/_helpers/main.php",
            "src/_helpers/path.php"
        ],
        "psr-4": {
            "Mini\\": "src/"
        }
    }
}
